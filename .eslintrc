{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "react-app",
  "globals": {
    "graphql": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": [0],
    "prettier/prettier": "error",
    "arrow-parens": ["error", "as-needed", { "requireForBlockBody": true }]
  },
  "parser": "babel-eslint"
}
